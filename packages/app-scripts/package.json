{
  "name": "@lcooper/app-scripts",
  "version": "0.2.1",
  "description": "Configuration and scripts for js web apps",
  "main": "./lib/index.js",
  "types": "./lib/index.d.ts",
  "bin": {
    "app-scripts": "./lib/bin/app-scripts.js"
  },
  "exports": {
    ".": "./lib/index.js",
    "./middleware": "./lib/middleware.js",
    "./package.json": "./package.json"
  },
  "files": [
    "lib",
    "!**/*.tsbuildinfo"
  ],
  "scripts": {
    "lint": "eslint .",
    "test": "tsc --noEmit",
    "clean": "rimraf lib",
    "build": "tsc -b tsconfig.json",
    "watch": "tsc -b tsconfig.json --watch",
    "prepublishOnly": "yarn test && yarn build"
  },
  "engines": {
    "node": ">=14"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/luciancooper/app-toolkit.git",
    "directory": "packages/app-scripts"
  },
  "keywords": [
    "webpack",
    "react",
    "bundler"
  ],
  "author": "Lucian Cooper <cooperlucian@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/luciancooper/app-toolkit/issues"
  },
  "homepage": "https://github.com/luciancooper/app-toolkit/tree/main/packages/app-scripts#readme",
  "dependencies": {
    "@babel/core": "^7.16.7",
    "@babel/plugin-transform-runtime": "^7.16.7",
    "@babel/preset-env": "^7.16.7",
    "@babel/preset-react": "^7.16.7",
    "@babel/preset-typescript": "^7.16.7",
    "@babel/runtime": "^7.16.7",
    "@lcooper/dev-server": "0.1.0",
    "@lcooper/webpack-messages": "0.1.0",
    "@pmmmwh/react-refresh-webpack-plugin": "^0.5.4",
    "@svgr/webpack": "^6.1.2",
    "babel-loader": "^8.2.3",
    "browserslist": "^4.19.1",
    "chalk": "^4.1.0",
    "cosmiconfig": "^7.0.0",
    "css-loader": "^6.5.1",
    "css-minimizer-webpack-plugin": "^3.3.1",
    "eslint": "^8.6.0",
    "eslint-webpack-plugin": "^3.1.1",
    "fork-ts-checker-webpack-plugin": "^6.5.0",
    "fs-extra": "^9.0.1",
    "globby": "^11.0.1",
    "html-webpack-plugin": "^5.5.0",
    "mini-css-extract-plugin": "^2.6.0",
    "mini-svg-data-uri": "^1.2.3",
    "postcss": "^8.4.5",
    "postcss-loader": "^6.2.1",
    "postcss-preset-env": "^7.2.0",
    "react-refresh": "^0.11.0",
    "resolve-url-loader": "^4.0.0",
    "sass": "^1.45.2",
    "sass-loader": "^12.4.0",
    "style-loader": "^3.3.1",
    "stylelint": "^14.2.0",
    "stylelint-webpack-plugin": "^3.1.0",
    "svgo": "^2.8.0",
    "svgo-loader": "^3.0.0",
    "terser-webpack-plugin": "^5.3.0",
    "webpack": "^5.65.0"
  }
}
