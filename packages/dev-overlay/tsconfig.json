{
  "extends": "../../tsconfig.json",
  "compilerOptions": {
    "target": "ES2021",
    "module": "esnext",
    "lib": [
      "ES2020",
      "DOM",
      "DOM.Iterable"
    ],
    "jsx": "react-jsx",
    "jsxImportSource": "src/overlay",
    "emitDeclarationOnly": true,
    "composite": true,
    "baseUrl": ".",
    "rootDir": "src",
    "outDir": "dist",
    "tsBuildInfoFile": "dist/.tsbuildinfo"
  },
  "include": [
    "src"
  ],
  "exclude": [],
  "references": [
    { "path": "../webpack-messages" }
  ]
}
